{
  "name": "@fluxbase/sdk-react",
  "version": "2026.2.3",
  "description": "React hooks for Fluxbase SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "fluxbase",
    "react",
    "hooks",
    "database",
    "realtime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fluxbase-eu/fluxbase"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@fluxbase/sdk": "^2026.2.3",
    "@tanstack/react-query": "^5.90.21",
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@fluxbase/sdk": "file:../sdk",
    "@tanstack/react-query": "^5.90.21",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@vitest/coverage-v8": "^4.0.18",
    "jsdom": "^28.1.0",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tsup": "^8.5.1",
    "typedoc": "^0.28.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=24.14.0"
  }
}
